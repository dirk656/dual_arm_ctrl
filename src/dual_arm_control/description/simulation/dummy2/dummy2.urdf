    <inertial>
      <!-- 总基座惯性参数（适配2kg质量+0.4*0.2*0.1m尺寸） -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.008333" ixy="0.0" ixz="0.0" 
               iyy="0.028333" iyz="0.0" izz="0.033333"/>
    </inertial>
    <visual>
      <!-- 总基座可视化（长方体，方便RViz定位） -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.6 0.2 0.1"/> <!-- 长x=0.6m（容纳双臂），宽y=0.2m，高z=0.1m -->
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <!-- 总基座碰撞体（与可视化一致） -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
    </collision>
  </link>

